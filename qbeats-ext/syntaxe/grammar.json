{
  "scopeName": "source.qb",
  "patterns": [
    {
      "include": "#keywords"
    }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "match": "\\b(Music|Tempo|TimeSignature|Ticks|DefaultOctave|DefaultNoteType|Tracks|Track|Instrument|Notes|KeyboardBinding|Bindings|Patterns|Key|PitchBend)\\b",
          "name": "keyword.qb"
        },
        {
          "begin": "\"",
          "end": "\"",
          "name": "string.qb"
        }
      ]
    }
  }
}
